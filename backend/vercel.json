{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/send-mail",
        "dest": "/backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/dist/$1"
      }
    ]
  }